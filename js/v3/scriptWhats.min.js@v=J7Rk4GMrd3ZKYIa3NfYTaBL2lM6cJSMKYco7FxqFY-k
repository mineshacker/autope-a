function SourceOfTheSearch(){var n,t,r=new URLSearchParams(window.location.search),i=r.get("x");switch(i){case"fb":n=1;t="Digite qual é o seu Veículo:";break;case"tk":n=2;t="Tik Tok: ";break;case"gg":n=3;t="Digite qual é o seu Carro:";break;case"gg1":n=3;t="Digite qual é o seu Carro:";break;case"gg2":n=4;t="Google Conversão: ";break;case"mb":n=5;t="My Business: ";break;default:n=i?9:0;t="Digite qual é o seu carro:"}return{origin_code:n,origin_msg:t}}function getRandomIntInclusive(n,t){return n=Math.ceil(n),t=Math.floor(t),Math.floor(Math.random()*(t-n+1))+n}function click0800(){var t=SourceOfTheSearch(),n=new FormData;n.append("click_origin",1);n.append("origin",t.origin_code);fetch("Get/Landing/SaveClickPhone",{method:"POST",body:n}).then(function(n){return n.json()}).then(function(n){n.status!="Sucesso"&&msgBox("Erro:",n.Mensagem)}).catch(function(n){console.error("Erro:",n)});closeModal()}function OpenWhatsApp(n,t){n.preventDefault();let r=SourceOfTheSearch(),i=new FormData;i.append("click_origin",0);i.append("origin",r.origin_code);fetch("Get/Landing/GetPhone",{method:"POST",body:i}).then(function(n){return n.json()}).then(function(n){if(n.status=="Sucesso"){var i="Cupom de desconto: "+n.codigo+" \nAperte enviar >> \n"+r.origin_msg;whatsappMessage=encodeURIComponent(i);window.location.assign(t.href+"&text="+whatsappMessage)}else msgBox("Erro:",n.Mensagem)}).catch(function(n){console.error("Erro:",n)})}